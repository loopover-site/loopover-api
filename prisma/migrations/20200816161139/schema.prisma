generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model knex_migrations {
  id             Int       @default(autoincrement()) @id
  name           String?
  batch          Int?
  migration_time DateTime?
}

model knex_migrations_lock {
  index     Int  @default(autoincrement()) @id
  is_locked Int?
}

model leaderboards {
  id             Int     @default(autoincrement()) @id
  category       String
  subCategory    String
  date           Int
  evidence       String
  userHasAccount Boolean
  username       String
  time           Int     @default(0)
}

model sessions {
  id      Int    @default(autoincrement()) @id
  key     String
  maxAge  Int
  session Json
}

model users {
  id       Int    @default(autoincrement()) @id
  username String @unique
  password String
  role     String
}
